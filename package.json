{
    "name": "medcreds-api",
    "version": "1.0.0",
    "description": "Koa Node Server",
    "main": "server.js",
    "scripts": {
        "watch-test": "DISABLE_REQUEST_LOGGING=${DISABLE_REQUEST_LOGGING:-1} jest --watchAll --detectOpenHandles",
        "test": "jest --passWithNoTests --runInBand --detectOpenHandles --forceExit --reporters default --reporters jest-junit",
        "dev": "nodemon server.js",
        "start": "node server.js",
        "cluster": "node cluster.js",
        "db-migrate": "knex migrate:latest",
        "db-seed": "knex seed:run",
        "db-rollback": "knex migrate:rollback",
        "db-init": "npm run db-migrate; npm run db-seed",
        "rebuild-deps": "npm rebuild bcrypt --build-from-source",
        "docs": "docsify serve",
        "process-tenants": "node ./scripts/process-tenants.js",
        "test-redis": "node ./scripts/test-redis.js"
    },
    "jest": {
        "verbose": true
    },
    "jest-junit": {
        "suiteName": "api functional tests",
        "outputDirectory": ".",
        "outputName": "junit.xml",
        "uniqueOutputName": "false",
        "classNameTemplate": "{classname}-{title}",
        "titleTemplate": "{classname}-{title}",
        "ancestorSeparator": " â€º ",
        "usePathForSuiteName": "true"
    },
